build: [
  ["corebuild" "src/slack_rtm.native"
               "-pkgs" "async_ssl,websocket.async,cohttp.async,yojson"]
  ["cp" "slack_rtm.native" "slack-rtm"]
]
install: [
  ["opam-installer" "--prefix=%{prefix}%" "slack_rtm.install"]
]
