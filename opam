  "ocamlfind"        { build }
  "core"             { >= "113.33" }
  "async_ssl"        { >= "113.33" }
  "websocket"        { >= "2.2" }
  "yojson"           { >= "1.3.2" }
  "cohttp"           { >= "0.21.0" }
]
